{
  "expo": {
    "name": "word-wizard",
    "slug": "word-wizard",
    "version": "0.0.2",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#666699"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.lojhiic.wordwizard",
      "supportsTablet": true
    },
    "android": {
      "package": "com.lojhiic.wordwizard",
      "versionCode": 2,
      "permissions": [],
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#666699"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
